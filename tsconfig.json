{
  "extends": "@deephaven/tsconfig",
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@deephaven/iris-grid/dist/*": [
        "./packages/iris-grid/src/*"
      ],
      "@deephaven/*": [
        "./packages/*/src"
      ]
    }
  },
  "include": [
    "packages/*/src/*.ts"
  ],
  "exclude": [
    "node_modules",
    "packages/*/dist"
  ]
}