  },
  "homepage": ".",
  "main": "public/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts --max-old-space-size=4096 start",
    "build-js": "react-scripts --max-old-space-size=4096 build",
    "build": "npm-run-all build-js clean-mocks",
    "lint-src": "eslint --ext=js --ext=jsx ./src --fix",
    "test:dev:lint:js": "eslint --ext=js --ext=jsx --cache ./src",
    "test:dev:lint:scss": "stylelint --config=.stylelintrc --cache \"./src/**/*.scss\"",
    "test:dev:lint": "run-s test:dev:lint:*",
    "test": "react-scripts --max-old-space-size=4096 test",
    "test:ci": "react-scripts --max-old-space-size=4096 test --watchAll=false",
    "test:xml-report": "cross-env-shell JEST_JUNIT_OUTPUT=\"${GRADLE_ROOT:-.}/build/reports/tests/js/TEST-jest-junit.xml\" node node_modules/react-scripts/bin/react-scripts.js --max-old-space-size=4096 test --watchAll=false --reporters=jest-junit",
    "clean-mocks": "rm -rf build/__mocks__",
    "eject": "react-scripts eject",
    "update-licenses": "npm-license > licenses.txt",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@deephaven/eslint-config": "^0.1.3",
