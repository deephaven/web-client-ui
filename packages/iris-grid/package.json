{
  "name": "@deephaven/iris-grid",
  "version": "0.11.2",
  "description": "Deephaven Iris Grid",
  "author": "Deephaven Data Labs LLC",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/deephaven/web-client-ui.git",
    "directory": "packages/iris-grid"
  },
  "main": "dist/index.js",
  "source": "src/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build-dev": "cross-env NODE_ENV=development run-p build:*",
    "babel": "babel ./src --out-dir ./dist --extensions \".ts,.tsx,.js,.jsx\" --source-maps --root-mode upward",
    "sass": "sass ./src:./dist",
    "build:babel": "npm run babel",
    "build:sass": "npm run sass",
    "watch": "run-p watch:*",
    "watch:babel": "npm run babel -- -w --skip-initial-build",
    "watch:sass": "npm run sass -- --watch --update",
    "prestart": "npm run build-dev",
    "start": "cross-env NODE_ENV=development npm run watch"
  },
  "dependencies": {
    "@deephaven/grid": "^0.11.2",
    "@deephaven/icons": "^0.11.2",
    "@deephaven/jsapi-shim": "^0.11.2",
    "@deephaven/react-hooks": "^0.11.2",
    "@deephaven/utils": "^0.11.2",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "classnames": "^2.3.1",
    "deep-equal": "^2.0.5",
    "lodash.clamp": "^4.0.3",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "memoize-one": "^5.1.1",
    "memoizee": "^0.4.15",
    "monaco-editor": "^0.27.0",
    "prop-types": "^15.7.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-transition-group": "^2.9.0",
    "shortid": "^2.2.16",
    "web-streams-polyfill": "^2.1.0"
  },
  "peerDependencies": {
    "@deephaven/components": "^0.3.1",
    "@deephaven/log": "^0.3.1",
    "react": "^17.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@deephaven/components": "^0.11.2",
    "@deephaven/log": "^0.11.2",
    "@deephaven/mocks": "^0.11.2",
    "@deephaven/tsconfig": "^0.11.2",
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-transition-group": "^4.4.0",
    "cross-env": "^7.0.2",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.39.0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.scss"
  ],
  "publishConfig": {
    "access": "public"
  }
}
