{
  "name": "@deephaven/react-hooks",
  "version": "0.9.0",
  "description": "Deephaven React hooks library",
  "author": "Deephaven Data Labs LLC",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/deephaven/web-client-ui"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build-dev": "cross-env NODE_ENV=production run-p build:*",
    "babel": "babel ./src --out-dir ./dist --extensions \".ts,.tsx,.js,.jsx\" --source-maps --root-mode upward",
    "types": "tsc -b",
    "build:babel": "npm run babel",
    "watch": "run-p watch:*",
    "watch:babel": "npm run babel -- -w --skip-initial-build",
    "clean": "run-p clean:*",
    "clean:types": "tsc -b --clean",
    "clean:build": "rimraf ./dist",
    "prestart": "npm run build-dev",
    "start": "cross-env NODE_ENV=development npm run watch"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@deephaven/tsconfig": "^0.9.0",
    "@types/react": "^16.14.8",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "rimraf": "^3.0.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
