{
  "name": "@deephaven/golden-layout",
  "version": "0.3.0",
  "author": "Deephaven Data Labs LLC",
  "license": "Apache-2.0",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-insert": "^0.5.0",
    "gulp-uglify": "^1.5.3",
    "karma": "0.13.*",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "~0.1.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.32.13"
  },
  "description": "A multi-screen javascript Layout manager",
  "main": "./dist/goldenlayout.js",
  "types": "./index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "jquery": "^3.5.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clean build-steps",
    "build-dev": "cross-env NODE_ENV=development run-s clean build-steps",
    "build-steps": "run-p build:*",
    "sass": "sass ./src/scss:./dist/css -s compressed",
    "build:gulp": "gulp build",
    "build:sass": "npm run sass",
    "watch": "run-p watch:*",
    "watch:gulp": "gulp dev",
    "watch:sass": "npm run sass -- --watch --update",
    "clean": "rimraf ./dist ./src/css",
    "start": "cross-env NODE_ENV=development npm run watch",
    "test": "karma start karma.conf.js",
    "test:ci": "karma start karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deephaven/web-client-ui"
  },
  "keywords": [
    "layout manager",
    "javascript",
    "docker",
    "layout",
    "popouts"
  ],
  "files": [
    "dist"
  ],
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "goldenlayout.js",
        "goldenlayout.min.js"
      ]
    },
    {
      "basePath": "/src/css/",
      "files": [
        "goldenlayout.base.css",
        "goldenlayout-dark-theme.css",
        "goldenlayout-light-theme.css"
      ]
    },
    {
      "basePath": "/",
      "files": [
        "typings.json"
      ]
    }
  ],
  "jspm": {
    "main": "dist/goldenlayout",
    "format": "global",
    "registry": "jspm",
    "dependencies": {
      "jquery": "^2.1.0"
    },
    "shim": {
      "dist/goldenlayout": [
        "jquery"
      ]
    }
  }
}
