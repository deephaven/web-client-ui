{
  "name": "@deephaven/golden-layout",
  "version": "0.19.0",
  "author": "Deephaven Data Labs LLC",
  "license": "Apache-2.0",
  "description": "A multi-screen javascript Layout manager",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "type": "module",
  "dependencies": {
    "jquery": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^17.x",
    "react-dom": "^17.x"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build-dev": "cross-env NODE_ENV=development run-p build:*",
    "sass": "sass ./src/scss:./dist/css -s compressed",
    "build:babel": "npm run babel",
    "build:sass": "npm run sass",
    "babel": "babel ./src --out-dir ./dist --extensions \".ts,.js\" --source-maps --root-mode upward",
    "watch": "run-p watch:*",
    "watch:babel": "npm run babel -- -w --skip-initial-build",
    "watch:sass": "npm run sass -- --watch --update",
    "clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=development npm run watch",
    "prestart": "npm run build-dev",
    "test": "karma start karma.conf.cjs",
    "test:ci": "karma start karma.conf.cjs --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deephaven/web-client-ui.git",
    "directory": "packages/golden-layout"
  },
  "keywords": [
    "layout manager",
    "javascript",
    "docker",
    "layout",
    "popouts"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "esmify": "^2.1.1",
    "karma-browserify": "^8.1.0",
    "tsify": "^5.0.4",
    "watchify": "^4.0.0"
  }
}
