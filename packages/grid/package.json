{
  "name": "@deephaven/grid",
  "version": "0.11.4",
  "description": "Deephaven React grid component",
  "author": "Deephaven Data Labs LLC",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/deephaven/web-client-ui.git",
    "directory": "packages/grid"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build-dev": "cross-env NODE_ENV=development run-p build:*",
    "babel": "babel ./src --out-dir ./dist --extensions \".ts,.tsx,.js,.jsx\" --source-maps --root-mode upward",
    "sass": "sass ./src:./dist",
    "build:babel": "npm run babel",
    "build:sass": "npm run sass",
    "watch": "run-p watch:*",
    "watch:babel": "npm run babel -- -w --skip-initial-build",
    "watch:sass": "npm run sass -- --watch --update",
    "prestart": "npm run build-dev",
    "start": "cross-env NODE_ENV=development npm run watch"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@deephaven/tsconfig": "file:../tsconfig"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "color-convert": "^2.0.1",
    "event-target-shim": "^6.0.2",
    "lodash.clamp": "^4.0.3",
    "memoize-one": "^5.1.1",
    "memoizee": "^0.4.15",
    "prop-types": "^15.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
