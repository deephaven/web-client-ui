{
  "name": "@deephaven/file-explorer",
  "version": "0.9.0",
  "description": "Deephaven File Explorer React component",
  "author": "Deephaven Data Labs LLC",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/deephaven/web-client-ui"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build-dev": "cross-env NODE_ENV=development run-p build:*",
    "babel": "babel ./src --out-dir ./dist --extensions \".ts,.tsx,.js,.jsx\" --source-maps --root-mode upward",
    "sass": "sass ./src:./dist",
    "types": "tsc -b",
    "build:babel": "npm run babel",
    "build:sass": "npm run sass",
    "watch": "run-p watch:*",
    "watch:babel": "npm run babel -- -w --skip-initial-build",
    "watch:sass": "npm run sass -- --watch --update",
    "clean": "run-p clean:*",
    "clean:types": "tsc -b --clean",
    "clean:build": "rimraf ./dist",
    "prestart": "npm run build-dev",
    "start": "cross-env NODE_ENV=development npm run watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@deephaven/icons": "^0.9.0",
    "@deephaven/storage": "^0.9.0",
    "@deephaven/utils": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "classnames": "^2.3.1",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.7.2",
    "reactstrap": "^8.4.1",
    "webdav": "^4.6.1"
  },
  "peerDependencies": {
    "@deephaven/components": "^0.3.1",
    "@deephaven/log": "^0.3.1",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@deephaven/components": "^0.9.0",
    "@deephaven/log": "^0.9.0",
    "@deephaven/tsconfig": "^0.9.0",
    "@storybook/addon-actions": "^6.2.3",
    "@storybook/addon-essentials": "^6.2.3",
    "@storybook/addon-links": "^6.2.3",
    "@storybook/react": "^6.2.3",
    "@types/react": "^16.14.8",
    "cross-env": "^7.0.2",
    "follow-redirects": ">=1.14.7",
    "identity-obj-proxy": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "rimraf": "^3.0.2",
    "sass": "1.32.13"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
