    "memoizee": "^0.4.15",
    "plotly.js": "^2.18.0",
    "prop-types": "^15.7.2",
    "react-plotly.js": "^2.6.0"
  },
  "peerDependencies": {
    "react": "^17.x"
  },
  "devDependencies": {
    "@deephaven/mocks": "file:../mocks",
