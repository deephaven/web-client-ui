{
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "!{projectRoot}/dist/**/*",
      "!{projectRoot}/build/**/*",
      "!{projectRoot}/css/**/*",
      "!{projectRoot}/tsconfig.tsbuildinfo"
    ],
    "build": [
      "default",
      "!{projectRoot}/**/*.test.*",
      "!{projectRoot}/test/**/*",
      "!{projectRoot}/**/__mocks__/**/*"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["build", "^build", "{workspaceRoot}/babel.config.js"],
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/build", "{projectRoot}/dist"],
      "cache": true
    },
    "build:babel": {
      "inputs": [
        "build",
        "!{projectRoot}/**/*.scss",
        "{workspaceRoot}/babel.config.js"
      ],
      "outputs": [
        "{projectRoot}/dist/**/*.js",
        "{projectRoot}/dist/**/*.js.map"
      ],
      "cache": true
    },
    "build:sass": {
      "inputs": ["{projectRoot}/**/*.scss"],
      "outputs": [
        "{projectRoot}/css",
        "{projectRoot}/dist/**/*.css",
        "{projectRoot}/dist/**/*.css.map"
      ],
      "cache": true
    },
    "svgo-dh": {
      "outputs": ["{projectRoot}/dist/svg/dh"],
      "cache": true
    },
    "svgo-vs": {
      "outputs": ["{projectRoot}/dist/svg/vs"],
      "cache": true
    },
    "build:icons": {
      "outputs": ["{projectRoot}/dist/svg"],
      "cache": true
    },
    "build:js": {
      "outputs": ["{projectRoot}/dist", "!{projectRoot}/dist/svg"],
      "cache": true
    }
  }
}
